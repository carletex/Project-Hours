<table class="table table-striped">
    <thead>
        <tr>
            <th>Project</th>
            <th>User</th>
            <th>Description</th>
            <th>Duration</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td>{{ entity.project }} ({{ entity.project.client.name }})</td>
            <td>{{ entity.user }}</td>
            <td>{{ entity.description }}</td>
            <td>{{ entity.duration }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
